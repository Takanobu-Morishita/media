<% if current_user.favorite?(post) %>
  <td>
    <%= button_to 'いいね！を外す', post_favorite_path(post_id: post.id, id: current_user.favorites.find_by(post_id: post.id)), method: :delete %>
  </td>
<% else %>
  <td><%= button_to 'いいね！', post_favorites_path(post_id: post.id), method: :post %></td>
<% end %>